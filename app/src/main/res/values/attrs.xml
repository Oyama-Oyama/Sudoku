<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DataView">
        <attr name="vTitle" format="reference"/>
        <attr name="vMsgColor" format="reference"/>
    </declare-styleable>

    <declare-styleable name="ChartView">
        <attr name="chartType" format="enum">
            <enum name="totalWinRate" value="1"/>
            <enum name="type15DayCount" value="2"/>
            <enum name="type15DayWinRate" value="3"/>
            <enum name="weeklyWinRate" value="4"/>
        </attr>
    </declare-styleable>

</resources>